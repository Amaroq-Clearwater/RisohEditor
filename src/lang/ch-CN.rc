IDM_MAIN MENU
{
    POPUP "文件(&F)"
    {
        MENUITEM "新建(&N)\tCtrl+N", CMDID_NEW
        MENUITEM SEPARATOR
        MENUITEM "打开(&O)...\tCtrl+O", CMDID_OPEN
        MENUITEM "另存为(&A)...\tCtrl+S", CMDID_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "加载(&I)...\tCtrl+I", CMDID_IMPORT
        MENUITEM "加载窗口类DLL(&W)...", CMDID_LOADWCLIB
        MENUITEM SEPARATOR
        MENUITEM "&ID 关联...", CMDID_IDASSOC
        MENUITEM "重新加载(&R) &resource.h...\tCtrl+R", CMDID_LOADRESH
