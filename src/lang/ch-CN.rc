        MENUITEM "添加资源项目...", CMDID_ADDRES
        MENUITEM SEPARATOR
        MENUITEM "替换图标...", CMDID_REPLACEICON
        MENUITEM "替换光标...", CMDID_REPLACECURSOR
        MENUITEM "替换位图文件...", CMDID_REPLACEBITMAP
        MENUITEM "替换二进制资源项目...", CMDID_REPLACEBIN
        MENUITEM SEPARATOR
        MENUITEM "导出图标...", CMDID_EXTRACTICON
        MENUITEM "导出光标...", CMDID_EXTRACTCURSOR
        MENUITEM "导出位图文件...", CMDID_EXTRACTBITMAP
        MENUITEM "导出二进制资源文件...", CMDID_EXTRACTBIN
        MENUITEM SEPARATOR
        MENUITEM "效果测试(&T)", CMDID_TEST
        MENUITEM SEPARATOR
        MENUITEM "复制为别名(&N)...", CMDID_COPYASNEWNAME
        MENUITEM "复制为别语言(&L)...", CMDID_COPYASNEWLANG
        MENUITEM SEPARATOR
        MENUITEM "删除资源项目(&D)\tDel", CMDID_DELETERES
    }
    POPUP "搜索(&S)"
    {
        MENUITEM "查询(&F)...\tCtrl+F", CMDID_FIND
        MENUITEM SEPARATOR
        MENUITEM "向上搜索(&U)", CMDID_FINDUPWARD
        MENUITEM "向下搜索(&D)", CMDID_FINDDOWNWARD
        MENUITEM SEPARATOR
        MENUITEM "替换(&R)...\tCtrl+H", CMDID_REPLACE
        MENUITEM SEPARATOR
        MENUITEM "項目搜索(&I)...\tCtrl+M", CMDID_ITEMSEARCH
    }
    POPUP "界面显示(&V)"
    {
        MENUITEM "状态栏(&S)", CMDID_STATUSBAR
        MENUITEM "二进制模式(&B)", CMDID_BINARYPANE
        MENUITEM SEPARATOR
        MENUITEM "资源ID列表(&R)", CMDID_IDLIST
        MENUITEM SEPARATOR
        MENUITEM "使用&CONTROL语句 ", CMDID_ALWAYSCONTROL
        MENUITEM "不使用&ID宏定义", CMDID_HIDEIDMACROS
    }
    POPUP "帮助(&H)"
    {
        MENUITEM "打开英文 &README.txt", CMDID_OPENREADME
        MENUITEM "打开日文 README&JP.txt", CMDID_OPENREADMEJP
        MENUITEM SEPARATOR
        MENUITEM "打开 &LICENSE.txt", CMDID_OPENLICENSE
        MENUITEM SEPARATOR
        MENUITEM "关于(&A)...", CMDID_ABOUT
    }
}

